                </dul:hasRegion>
            </owl:NamedIndividual>
        </SOMA:hasShape>
        <SOMA:hasLocalization>
            <owl:NamedIndividual>
                <rdf:type rdf:resource="&SOMA;Localization"/>
                <SOMA:hasSpaceRegion>
                    <owl:NamedIndividual>
                        <rdf:type rdf:resource="&SOMA;6DPose"/>
